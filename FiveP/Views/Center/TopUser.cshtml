@using FiveP.Models;
@model List<User>
<div class="card gedf-card">
    <div class="card-body">
        <h4 class="card-title">Top thành viên</h4>
        @foreach (var item in Model)
        {
            <p class="card-text">
                <div class="row">
                    <div class="col-2">
                        <img class="rounded-circle" width="45" src="~/Content/LayoutCenter/ImgUser/user1.jpg" alt="">
                    </div>
                    <div class="col-10">
                        <a href="#" class="h5 m-0">@item.user_nicename</a>
                        <div class="h7 text-muted">@item.user_vip_medal<i class="fab fa-galactic-senate" style="color:orangered;font-size:28px"></i> @item.user_gold_medal<i class="fas fa-medal" style="color:yellow"></i>  @item.user_silver_medal<i class="fas fa-medal" style="color:silver"></i>  @item.user_bronze_medal<i class="fas fa-medal" style="color:#e88d15"></i> </div>
                    </div>
                </div>
                
                
            </p>
        }
        <span style="font-size:28px">...</span>
    </div>
</div>

